{
  "name": "sequential-async-foreach",
  "version": "1.0.4",
  "description": "An NPM package to properly handle using async functions in a forEach fashion (unlike Native forEach). Uses proper Promises and is light and efficient.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "rimraf dist && tsc && tsc -p tsconfig.esm.json",
    "format-check": "prettier --check .",
    "format": "prettier --write .",
    "test": "jest --coverage --passWithNoTests",
    "lint": "eslint ./src --ext .tsx,.ts",
    "type-check": "tsc --noEmit",
    "clean-packages": "rimraf node_modules && rimraf package-lock.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dkershner6/sequential-async-foreach.git"
  },
  "keywords": [
    "await",
    "sequential",
    "async",
    "forEach",
    "Promise"
  ],
  "author": "Derek Kershner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dkershner6/sequential-async-foreach/issues"
  },
  "homepage": "https://github.com/dkershner6/sequential-async-foreach#readme",
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "await-wait": "^1.1.7",
    "cross-env": "^7.0.3",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist/*"
  ],
  "dependencies": {}
}
